<?xml version="1.0" encoding="UTF-8" ?>
<!-- Valid 한 설정 xml을 만들기 위해 DTD를 추가해 줍니다. -->
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <typeAliases>
        <typeAlias alias="user" type="com.spring.vo.User"/>
        <typeAlias alias="sqlBuilder" type="com.spring.vo.SQLBuilder"/>
    </typeAliases>
    
    <mappers>
        <mapper resource="META-INF/mapper/user.xml" />
        <mapper resource="META-INF/mapper/sql.xml" />
    </mappers>
    
</configuration>